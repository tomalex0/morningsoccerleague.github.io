{
  "name": "morningsoccerleague.github.io",
  "private": true,
  "description": "Morning Soccer League Web Codebase using Gatsby",
  "version": "0.4.2",
  "author": "Thomas Alexander <tomalex0@gmail.com>",
  "dependencies": {
    "@tailwindcss/forms": "^0.2.1",
    "autoprefixer": "^10.2.4",
    "gatsby": "^2.32.9",
    "gatsby-plugin-google-analytics": "^2.11.0",
    "gatsby-plugin-image": "^0.7.2",
    "gatsby-plugin-manifest": "^2.12.1",
    "gatsby-plugin-offline": "^3.10.2",
    "gatsby-plugin-postcss": "^3.7.0",
    "gatsby-plugin-react-helmet": "^3.10.0",
    "gatsby-plugin-sharp": "^2.14.3",
    "gatsby-plugin-sitemap": "^2.12.0",
    "gatsby-plugin-tinacms": "^0.37.0",
    "gatsby-plugin-webpack-bundle-analyser-v2": "^1.1.20",
    "gatsby-source-filesystem": "^2.11.1",
    "gatsby-tinacms-git": "^0.37.0",
    "gatsby-tinacms-json": "^0.37.0",
    "gatsby-tinacms-remark": "^0.37.0",
    "gatsby-transformer-json": "^2.11.0",
    "gatsby-transformer-remark": "^2.16.1",
    "gatsby-transformer-sharp": "^2.12.0",
    "postcss": "^8.2.6",
    "prop-types": "^15.7.2",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-helmet": "^6.1.0",
    "styled-components": "^5.2.1",
    "tailwindcss": "^2.0.3",
    "typescript": "^4.2.2"
  },
  "devDependencies": {
    "@babel/preset-react": "^7.12.13",
    "@cypress/code-coverage": "^3.9.2",
    "@typescript-eslint/parser": "^4.16.1",
    "babel-plugin-istanbul": "^6.0.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "cypress": "^6.6.0",
    "eslint": "^7.21.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-config-react-app": "^6.0.0",
    "eslint-plugin-cypress": "^2.11.2",
    "eslint-plugin-prettier": "^3.3.1",
    "istanbul-lib-coverage": "^3.0.0",
    "mochawesome": "^6.2.2",
    "mochawesome-merge": "^4.2.0",
    "mochawesome-report-generator": "^5.2.0",
    "nyc": "^15.1.0",
    "prettier": "2.1.2",
    "transform-class-properties": "^1.0.0-beta"
  },
  "keywords": [
    "gatsby"
  ],
  "license": "0BSD",
  "scripts": {
    "build": "gatsby build",
    "lint": "eslint . --ignore-path .prettierignore",
    "lint:fix": "eslint .  --fix",
    "build:prefix": "gatsby build --prefix-paths",
    "develop": "gatsby develop -H 0.0.0.0",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "start": "npm run develop",
    "serve": "gatsby serve -H 0.0.0.0",
    "clean": "gatsby clean",
    "check": "prettier --check \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "cy:open": "cypress open",
    "cy:run": "cypress run --spec 'cypress/integration/**/*'",
    "cy:run:file": "cypress run",
    "delete-e2e:reports": "rm cypress/results/e2e/* && rm e2e-mochawesome.json || true",
    "delete-unit:reports": "rm cypress/results/unit/* && rm unit-mochawesome.json || true",
    "preunit-test": "npm run delete-unit:reports",
    "pree2e-test": "npm run delete-e2e:reports",
    "e2e-test": "cypress run --spec  'cypress/integration/ui-test/**/*' --headless --env coverage=false && npm run e2e-merge && npm run e2e-report",
    "unit-test": "cypress run --config-file cypress-unit-test.json --spec  'cypress/integration/unit-test/**/*' --env coverage=true && npm run unit-merge && npm run unit-report",
    "e2e-merge": "mochawesome-merge cypress/results/e2e/*.json > e2e-mochawesome.json",
    "unit-merge": "mochawesome-merge cypress/results/unit/*.json > unit-mochawesome.json",
    "e2e-report": "npx marge e2e-mochawesome.json",
    "unit-report": "npx marge unit-mochawesome.json",
    "cy:run:report": "nyc cypress run",
    "test": "npm run unit-test && npm run e2e-test"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/morningsoccerleague/morningsoccerleague.github.io"
  },
  "bugs": {
    "url": "https://github.com/morningsoccerleague/morningsoccerleague.github.io/issues"
  }
}
